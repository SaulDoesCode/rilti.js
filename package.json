{
  "name": "rilti.js",
  "version": "0.8.0",
  "description": "small opinionated furture forward frontend framework",
  "main": "init.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sauldoescode/rilti.js.git"
  },
  "keywords": [
    "ES6",
    "ES7",
    "view",
    "library",
    "front-end",
    "framework",
    "proxy",
    "dom"
  ],
  "author": "Saul van der Walt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sauldoescode/rilti.js/issues"
  },
  "scripts": {
    "test": "./node_modules/.bin/karma start karma.conf.js",
    "build": "./node_modules/.bin/rollup -c && ./node_modules/.bin/standard --fix ./dist/rilti.js && ./node_modules/.bin/minify ./dist/rilti.js --outFile ./dist/rilti.min.js"
  },
  "standard": {
    "ignore": [
      "./dist/",
      "./examples/",
      "./examples/assets/",
      "*.min.js",
      "init.js"
    ]
  },
  "homepage": "https://github.com/sauldoescode/rilti.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "babel-minify": "^0.5.0-alpha.9",
    "chokidar": "^2.0.4",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "rollup": "^0.62.0",
    "standard": "^11.0.1"
  }
}
