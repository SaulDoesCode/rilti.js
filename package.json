{
  "name": "rilti",
  "version": "0.9.9",
  "description": "small opinionated furture forward frontend framework",
  "main": "./dist/rilti.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sauldoescode/rilti.js.git"
  },
  "keywords": [
    "ES6",
    "ES7",
    "view",
    "library",
    "front-end",
    "frontend",
    "toolkit",
    "components",
    "framework",
    "proxy",
    "dom"
  ],
  "author": "Saul van der Walt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sauldoescode/rilti.js/issues"
  },
  "scripts": {
    "test": "./node_modules/.bin/karma start karma.conf.js",
    "build": "./node_modules/.bin/rollup -c && ./node_modules/.bin/standard --fix ./dist/rilti.js && ./node_modules/.bin/terser --verbose -c -m -o ./dist/rilti.min.js -- ./dist/rilti.js"
  },
  "standard": {
    "ignore": [
      "./dist/",
      "./examples/",
      "./examples/assets/",
      "*.min.js",
      "init.js"
    ]
  },
  "homepage": "https://github.com/sauldoescode/rilti.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "chokidar": "^2.0.4",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "jasmine": "^3.2.0",
    "jasmine-core": "^3.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "rollup": "^0.67.1",
    "standard": "^12.0.1",
    "terser": "^3.10.11"
  }
}
